{
  "multi_factor_authentication": true,
  "password_complexity": {
    "minimum_length": 12,
    "require_uppercase": true,
    "require_lowercase": true,
    "require_numbers": true,
    "require_symbols": true
  },
  "session_management": {
    "session_timeout": 1800,
    "concurrent_sessions": 3,
    "secure_cookies": true
  },
  "role_based_access": true,
  "audit_logging": true
}